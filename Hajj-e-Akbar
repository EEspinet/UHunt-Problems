#include <iostream>
using namespace std;

int main() {
	
	string next;
	
	cin>>next;
	
	int counter = 1;
	while(next!="*")
	{
		if(next == "Hajj")
		{
			cout<<"Case "<<counter<<": Hajj-e-Akbar"<<endl;
		}
		else
		{
			cout<<"Case "<<counter<<": Hajj-e-Asghar"<<endl;
	    }
	    counter++;
	    cin>>next;
	}
	return 0;
}
